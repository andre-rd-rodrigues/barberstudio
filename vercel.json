{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/assets/:filePath",
      "destination": "/assets/:filePath",
      "permanent": true
    }
  ],
  "headers": [
    {
      "key": "Cache-Control",
      "value": "public, max-age=31536000, immutable"
    }
  ]
}
